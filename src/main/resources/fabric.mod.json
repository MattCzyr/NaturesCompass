{
  "schemaVersion": 1,
  "id": "naturescompass",
  "version": "${version}",

  "name": "Nature's Compass",
  "description": "Search for a biome and get information about it.",
  "authors": [
    "ChaosTheDude"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/natures-compass",
    "sources": "https://github.com/MattCzyr/NaturesCompass"
  },

  "license": "CC BY-NC-SA 4.0",
  "icon": "assets/naturescompass/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.chaosthedude.naturescompass.NaturesCompass"
    ],
    "client": [
      "com.chaosthedude.naturescompass.NaturesCompassClient"
    ]
  },
  "mixins": [
    "naturescompass.mixins.json"
  ],
  "accessWidener" : "naturescompass.accesswidener",
  "depends": {
    "fabricloader": ">=0.15.0",
    "fabric": "*",
    "minecraft": "~1.20.3",
    "java": ">=17"
  },
  "suggests": {
    "explorerscompass": "*"
  }
}
